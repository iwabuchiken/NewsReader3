<%= render "index_header" %>

<!-- <span id="top"></span> -->

<hr />

<%= render "index_admin_data" %>

<hr /> <!-- -------------------------------------------------- -->

	<% @category_names.each do |item| %>
	
		<%= link_to item, "##{item}" %> | 
	
	<% end %> 

<hr />

<% @objects.each_with_index do |cats, j| %>
	
	<span style="color: blue;">
<!-- 		<div name="<%#= @category_names[j] %>"> -->
		<div>
			<span id="<%= @category_names[j] %>">
				<%= @category_names[j] %> (<%= cats.size %>) |
			</span>
			
			<span>
				<%= link_to "Top", "#top" %>
			</span>
		</div>
		
		
	</span>
	<hr />
	<% cats.each_with_index do |item, i| %>
	
		<%#= i %> 
		<%= i + 1 %>.
		<%= link_to item.content, item['href'], :target => "_blank" %><br />
	
		<%# if i != 0 and i % 10 == 0 %>
		<% if (i < 12 and i > 8 and i % 9 == 0) or
				(i >= 12 and i % 10 == 0) 
				%>
		
			<hr />
		<%# elsif i % 10 == 0 %>
		
			<hr />
		
		<% end %>

	
	<% end %>
	
	end<hr />

<% end %>

<hr />
@objects.size: <%= @objects.size %>
