<h1>Genres</h1>

<table>
  <tr>
  	<th>Id</th>
    <th>Name</th>
    <th>Code</th>
  </tr>

<% @genres.each do |genre| %>
  <tr>
  	<td><%= genre.id %></td>
    <td><%= link_to genre.name, genre %></td>
    <td><%= genre.code %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Genre', new_genre_path %>

<hr />

<dl class="accordion">
	<dt>
		<span style="color: green">Admin data</span>
	</dt>
	
	<dd>
		session[:doc_num] => <%= session[:doc_num] %>
	</dd>
</dl>
